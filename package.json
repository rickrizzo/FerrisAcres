{
  "name": "FerrisAcres",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prestart": "npm run build:js",
    "start": "node ./bin/www",
    "serve": "nodemon ./bin/www",
    "build:js": "browserify src/app.js -t vueify -p [ vueify/plugins/extract-css -o public/stylesheets/styles.bundle.css ] -t babelify -o public/javascripts/app.bundle.js",
    "watch:js": "watchify src/app.js -t vueify -t babelify -p browserify-hmr -p [ vueify/plugins/extract-css -o public/stylesheets/styles.bundle.css ] -o public/javascripts/app.bundle.js",
    "dev": "concurrently \"npm run serve\" \"npm run watch:js\""
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.7",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "morgan": "~1.7.0",
    "pg": "^6.1.0",
    "pg-promise": "^5.4.5",
    "pug": "^2.0.0-beta6",
    "serve-favicon": "~2.3.0",
    "vue": "^2.1.3",
    "vue-router": "^2.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-hmr": "^0.3.5",
    "concurrently": "^3.1.0",
    "nodemon": "^1.11.0",
    "vue-hot-reload-api": "^2.0.6",
    "vueify": "^9.3.0",
    "watchify": "^3.7.0"
  }
}
